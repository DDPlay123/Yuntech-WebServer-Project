		  <div style="float:left">
				<a href="/web/list">All</a> |
				<a href="/web/week">Week</a> |
				<a href="/web/day">Day</a>
		  </div><br>
		  <table width='100%' border=0>
		  <tr style='text-align:left; background-color:#CCC'>
			  <th>name</th>
			  <th>date</th>
			  <th>Action</th>
		  </tr>
		  <% if (data) { %>
			  <% data.forEach(function(user){ %>
			  <tr>
			  	<td><%= user.name %></td>
			  	<td><%= user.date.toLocaleString("zh-TW",{hour12:false}) %></td>
				  <td>
					  <div style="float:left">
              <input type="button" value="Show" style="float:right" onclick="location.href='/web/map/<%= user.id %>'">
					  	<form method="post" action="/web/delete/<%= user.id %>" style="float:right">
					  		<input type="submit" name="delete" value='Delete' />
						  	<input type="hidden" name="_method" value="DELETE" />
						  </form>
					  </div>
				  </td>
			  </tr>
			  <% }) %>
		  <% } %>
